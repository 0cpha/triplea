<?xml version="1.0" ?>
<!DOCTYPE game SYSTEM "game.dtd">
<game>
	<info name="test" version="1.0.0" />
	<loader javaClass="games.strategy.engine.xml.TestGameLoader" />
	<map>
		<territory name="t1" />
		<territory name="t2" />
		<connection t1="t1" t2="t2" />
	</map>
	<resourceList>
		<resource name="gold" />
		<resource name="PUs" />
		<resource name="silver" />
	</resourceList>
	<playerList>
		<player name="p1" optional="false" />
		<player name="p2" optional="false" />
	</playerList>
	<gamePlay>
		<delegate name="d1" javaClass="games.strategy.engine.xml.TestDelegate" />
		<sequence>
			<step name="s1" delegate="d1" />
		</sequence>
	</gamePlay>
</game>
